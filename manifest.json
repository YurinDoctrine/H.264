{
  "name": "H.264",
  "version": "4.1.3",
  "manifest_version": 2,
  "description": "H.264 stream in YouTube instead VP9/AV01 (for more visit my GitHub page <)",
  "homepage_url": "https://github.com/YurinDoctrine/H.264",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "H.264 ",
    "default_popup": "options.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*",
        "https://*.youtube-nocookie.com/*",
        "https://*.youtu.be/*"
      ],
      "js": ["src/inject.ts", "src/content_script.ts", "src/options.ts"],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "default_locale": "en",
  "options_ui": {
    "page": "options.html"
  },
  "permissions": ["storage"]
}
